{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.go",
      "use": "@vercel/go"
    }
  ],
  "routes": [
    {
      "src": "/ws",
      "dest": "backend/main.go"
    },
    {
      "src": "/health",
      "dest": "backend/main.go"
    },
    {
      "src": "/(.*)",
      "dest": "/test-frontend/$1"
    }
  ]
}
